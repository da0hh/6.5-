#include <iostream>

using namespace std;



int main() {
    setlocale(LC_ALL, "RU");
    int hour;
    cout << "Введите, который час: "; cin >> hour;
     
    if (hour <= 0 or hour > 12) {      //американское время
        cout << "Error..."; 
        return 0;
    }
    int num = 0;
    while (num < hour) {
        cout << "Ку-ку!\n";
        num++;
    }
}
